<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Style>
		<Style.Resources>
			<Color x:Key="ThemeBackgroundColor">#1a191a</Color>
			<Color x:Key="ThemeBackgroundLowColor">#222</Color>
			<Color x:Key="ThemeControlLowColor">#201f21</Color>
			<Color x:Key="ThemeControlHighColor">#38393c</Color>

			<SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{DynamicResource ThemeBackgroundColor}"/>
			<SolidColorBrush x:Key="ThemeBackgroundLowBrush" Color="{DynamicResource ThemeBackgroundLowColor}"/>
			<SolidColorBrush x:Key="ThemeControlLowBrush" Color="{DynamicResource ThemeControlLowColor}"/>
			<SolidColorBrush x:Key="ThemeControlHighBrush" Color="{DynamicResource ThemeControlHighColor}"/>
		</Style.Resources>
	</Style>
</Styles>
